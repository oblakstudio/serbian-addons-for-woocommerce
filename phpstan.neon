parameters:
  level: 5
  paths:
    - config
    - src
    - woocommerce
    - serbian-addons-for-woocommerce.php
  excludePaths:
    analyse:
      - src/Lib/QR/QR_*.php
  bootstrapFiles:
    - constants.php
    - vendor/php-stubs/woocommerce-stubs/woocommerce-stubs.php
  ignoreErrors:
    -
      identifier: argument.type
      paths:
        - src/Utils/Payments.php
    -
      identifier: constant.notFound
      paths:
        - config/*
        - src/Modules/Admin/Services/Settings_Page.php
        - src/Modules/Gateway/Services/Gateway_Payment_Slip.php
        - src/Utils/Installer.php
        - src/Utils/Template_Extender.php
        - src/App.php
